  SORT FIELDS=(9,1,CH,A,11,44,CH,A),SIZE=E300
  RECORD TYPE=V,LENGTH=(150,,,150)
  MODS E15=(WVOLCOLX,400000,,N)
  OUTFIL FILES=1,INCLUDE=(9,1,CH,EQ,X'00',OR,10,1,CH,EQ,X'00')
  OUTFIL FILES=2,INCLUDE=(9,1,CH,EQ,X'00',OR,10,1,CH,EQ,X'01')
  OUTFIL FILES=3,INCLUDE=(9,1,CH,NE,X'00',AND,10,1,CH,GT,X'01')
